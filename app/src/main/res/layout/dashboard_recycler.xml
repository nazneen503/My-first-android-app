<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:id="@+id/rel_number_box"
        android:layout_width="172dp"
        android:layout_height="194dp"
        android:layout_marginLeft="@dimen/dim_10"
        android:layout_marginTop="@dimen/dim_20"
        android:background="@drawable/dash_box1">

        <ImageView
            android:id="@+id/img_number_b1"
            android:layout_width="@dimen/dim_110"
            android:layout_height="@dimen/dim_110"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/dim_20" />

        <ImageView
            android:id="@+id/img_numbers"
            android:layout_width="@dimen/dim_75"
            android:layout_height="@dimen/dim_90"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/dim_30"

            />

        <TextView
            android:id="@+id/txt_match"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/dim_140"
            android:gravity="center"
            android:textColor="@android:color/black"
            android:textSize="@dimen/dim_17" />
    </RelativeLayout>


</LinearLayout>